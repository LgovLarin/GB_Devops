  version: 2
  renderer: NetworkManager
  wifis:
    wlp2s0b1:
      dhcp4: false
      addresses: [ 192.168.0.23/24 ]
      gateway4: 192.168.0.1
      dhcp6: false
      nameservers:
           addresses: [ 1.1.1.1 ]
      access-points:
         "Larin":
           password: "2806INVlrn"

